@model WebShoppingOnline.Models.ThongKeModel

@if (Model != null)
{
    <div class="thongKe">
        <table class="table">
            <tr>
                <td>Hôm nay</td>
                <td>@Model.HomNay</td>
            </tr>
            <tr>
                <td>Hôm qua</td>
                <td>@Model.HomQua</td>
            </tr>
            <tr>
                <td>Tuần Này</td>
                <td>@Model.TuanNay</td>
            </tr>
            <tr>
                <td>Tuần Trước</td>
                <td>@Model.TuanTruoc</td>
            </tr>
            <tr>
                <td>Tháng này</td>
                <td>@Model.ThangNay</td>
            </tr>
            <tr>
                <td>Tháng trước</td>
                <td>@Model.ThangTruoc</td>
            </tr>
            <tr>
                <td>Tất cả</td>
                <td>@Model.TatCa</td>
            </tr>
            <tr>
                <td>Số người online</td>
                <td>@ViewBag.visitors_online</td>
            </tr>
        </table>
    </div>
}
